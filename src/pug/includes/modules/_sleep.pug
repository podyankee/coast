	.container
		.title
			span
				|sleep
				h3 sleep
		.holder__slider
			.holder__slider-item.holder-slider
				.holder-slider__head(style="background-image: url(assets/img/sleep-slider-bg.png)")
					.holder-slider__subtitle
						.subtitle
							.headline-left Resorts
							.subtitle__head Auberge #[span |] Australia
				.holder-slider__descr(style="background-image: url(assets/img/hammock.png)")
