						.holder-slider__info-subtitle Resort
						.holder-slider__info-title Auberge #[br] Australia
					.holder-slider__info-item
						.holder-slider__info-subtitle # of Nights
						.holder-slider__info-title
							.quantity
								input.nights(type='number' min='1' max='9' step='1' value='5')
							|Nights
					.holder-slider__info-item
						.holder-slider__info-subtitle # of Guests
						.holder-slider__info-title
							.quantity
								input.guests(type='number' min='1' max='9' step='1' value='4')
							|Guests
					.holder-slider__info-item
						.holder-slider__info-subtitle Pricing
						.holder-slider__info-title
							span.summ(data-nights="55" data-guests="25")
							| USD
							span.holder-pricing__span Per Night
				.holder-slider__btn
					a(href="#").btn
